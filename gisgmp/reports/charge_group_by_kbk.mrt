        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>charge</Name>
        <NameInSource>PostgreSQL</NameInSource>
        <Parameters isList="true" count="3">
          <value>bill_date_from,bill_date_from,7,0</value>
          <value>bill_date_to,bill_date_to,7,0</value>
          <value>countDraftAndSended,countDraftAndSended,2,0</value>
        </Parameters>
        <SqlCommand>select c.supplier_bill_id, c.bill_date, c.payer, c.purpose, c.totalamount, c.acknowledgment_status,
	p.name, p.lastname, p.middlename, p.orgname,
	o.sender_identifier, o.name as payee_name,
	cd.kbk
from charge as c
left join payer as p
	on c.payer = p.recid
left join organization as o
	on c.payee = o.recid
left join charge_data as cd
	on c.charge_data = cd.recid
where CAST(c.bill_date AS DATE) &gt;= CAST(@bill_date_from AS DATE)
	and CAST(c.bill_date AS DATE) &lt;= CAST(@bill_date_to AS DATE)
	and ((c.acknowledgment_status IN ('1', '2' , '3', '4', '5', '12', '13', '15', '16') and @countDraftAndSended = false)
        or (c.acknowledgment_status IN ('1', '2' , '3', '4', '5', '12', '13', '15', '16', '6', '11') and @countDraftAndSended = true))
	

</SqlCommand>
      </charge>
